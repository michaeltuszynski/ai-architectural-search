# Production Environment Configuration for AI Architectural Search

# Deployment Environment
ENVIRONMENT=production
DEBUG_MODE=false
LOG_LEVEL=WARNING

# Resource Optimization for Cloud
MEMORY_OPTIMIZATION=true
CPU_ONLY_MODE=true
ENABLE_MODEL_CACHING=true

# Performance Settings (optimized for cloud)
BATCH_SIZE=16
MAX_IMAGE_SIZE=256,256
MAX_RESULTS=5

# Search Configuration
SIMILARITY_THRESHOLD=0.1

# Model Configuration
CLIP_MODEL_NAME=ViT-B/32

# File Paths (use defaults)
IMAGE_DIRECTORY=images/
METADATA_FILE=image_metadata.json

# Web Interface
PAGE_TITLE=AI Architectural Search
